{
    "version": "1.0.0",
    "name": "knebekaise/sorted-linked-list",
    "description": "A PHP 8.2+ sorted linked list library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Knebekaise\\SortedLinkedList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Knebekaise\\SortedLinkedList\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpunit": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
